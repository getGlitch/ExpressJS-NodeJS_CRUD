<% for(var i=0; i < userData.length; i++){ %>
    <tr>
        <td>
            <%= i+1%>
        </td>
        <td>
            <%= userData[i].name%>
        </td>
        <td>
            <%= userData[i].email%>
        </td>
        <td>
            <%= userData[i].contact%>
        </td>
        <td>
            <%= userData[i].gender%>
        </td>
        <td class="btn-g">
            <a href="/update-user?id=<%=userData[i]._id%>" class="btn btn-success">Edit</a>
            <a class="btn btn-danger delete" data-id='<%=userData[i]._id%>'>Delete</a>
        </td>
    </tr>
    <% } %>